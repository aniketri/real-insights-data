{
  "functions": {
    "apps/web/src/app/api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/client/**"
    }
  },
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "cd apps/web && pnpm run build",
  "outputDirectory": "apps/web/.next"
} 