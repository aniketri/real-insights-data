{
  "name": "real-insights",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "npm run test --workspaces --if-present",
    "db:push": "dotenv -- pnpm --filter db exec prisma db push --skip-generate",
    "db:generate": "dotenv -- pnpm --filter db exec prisma generate",
    "db:studio": "dotenv -- pnpm --filter db exec prisma studio"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.2.5",
    "turbo": "^2.0.6",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.6.0",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "next-auth": "^4.24.11"
  }
}
